{
  "name": "custom-server-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_ENV=development ts-node --project tsconfig.server.json src/index.ts",
    "build:ts": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "yarn build:ts && yarn build:next",
    "start": "NODE_ENV=production node dist/index.js",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "NODE_ENV=development lingui compile",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@lingui/macro": "^3.7.2",
    "@lingui/react": "^3.7.2",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "babel-plugin-styled-components": "^1.12.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "graphql": "^15.5.0",
    "morgan": "^1.10.0",
    "next": "^12.0.8",
    "next-pwa": "^5.4.4",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.2.2",
    "react-i18next": "^11.8.10",
    "react-icons": "^4.2.0",
    "styled-components": "^5.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.8",
    "@babel/core": "^7.13.10",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@lingui/cli": "^3.7.2",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/graphql": "^14.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-i18next": "^8.1.0",
    "@types/react-icons": "^3.0.0",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-macros": "^3.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "4.0"
  },
  "license": "MIT"
}
